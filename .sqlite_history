.help
CREATE TABLE Ages(
 id INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL UNIQUE,
  name VARCHAR(128), 
  age INTEGER
);
.table
DELETE FROM Ages;
INSERT INTO Ages (name, age) VALUES ('Cecily', 40);
INSERT INTO Ages (name, age) VALUES ('Jacques', 19);
INSERT INTO Ages (name, age) VALUES ('Jacques', 32);
INSERT INTO Ages (name, age) VALUES ('Micah', 32);
INSERT INTO Ages (name, age) VALUES ('Pele', 16);
.table
SELECT * FROM Ages
;
.mode column;
.mode column
SELECT * FROM Ages;
SELECT hex(name || age) AS X FROM Ages ORDER BY X;
.quit
